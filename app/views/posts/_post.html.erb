<div id="<%= dom_id post %>" class = "post-container">
  <p>
    <%= post.title %>
  </p>

  <p>
    <%= post.description %>
  </p>

  <p>
    <%= post.keywords %>
  </p>
  <p>
      <%= link_to " ", post, class: "post link" %>
    </p>
    <% post.images.each do |image| %>
      <% image_tag (image, stye: o"object-fit : cover; width: 300px; heigt: 600px;")%>
    <% end %>
</div>
